<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="thegame.view.MenuViewController">
   <children>
      <Label fx:id="mainTitle" layoutX="52.0" layoutY="14.0" text="ColonyManagement">
         <font>
            <Font size="40.0" />
         </font>
      </Label>
      <Button fx:id="newButton" layoutX="202.0" layoutY="91.0" mnemonicParsing="false" onAction="#handleNewButton" text="New">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="continueButton" layoutX="184.0" layoutY="150.0" mnemonicParsing="false" onAction="#handleContinuteButton" text="Continue">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="206.0" layoutY="207.0" mnemonicParsing="false" onAction="#handleExitButtonAction" text="Exit">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
